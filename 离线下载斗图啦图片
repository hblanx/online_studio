import requests
import re
headers={
    "User-Agent":"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/81.0.4044.138 Safari/537.36"
    }
a=""
b=0
f=open("图片地址/text.txt","r",encoding="utf-8")
urls=f.readlines()
for url in urls:
    file_name = "图片"+str(b)+".jpg"
    b+=1
    url=url[:-1]
    print("打印%d"%b)
    response = requests.get(url,headers=headers)
    with open("图片/"+file_name,"wb")as f2:
        f2.write(response.content)
f.close()
